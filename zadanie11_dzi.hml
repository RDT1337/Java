<!DOCTYPE html>
<head>
<title> Proste Obliczenia</title>
<meta charset="utf-8">
</head>
<body>
<div style="color:red;font-size:50px;">
<script language="JavaScript">
var liczba1=prompt("d1=","");
var liczba2=prompt("d2=","");
var ile_sekund_dzi;
var d1_dzi=parseFloat(liczba1);
var d2_dzi=parseFloat(liczba2);
ile_sekund_dzi=(d2_dzi-d1_dzi)*24*3600;
document.write(" D1= " +d1_dzi+  "  D2= "+d2_dzi+"<br>");
document.write("Ile sekund: "+ile_sekund_dzi+"<br>");
</script>
</div>
</body>
</html> 
